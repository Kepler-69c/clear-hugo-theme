{
  "name": "clear",
  "version": "0.1.0",
  "description": "Clear is a minimalistic hugo theme by Kepler-69c",
  "main": "index.js",
  "repository": "https://github.com/Kepler-69c/clear-hugo-theme",
  "author": "Kepler-69c",
  "license": "MIT",
  "scripts": {
    "start": "hugo server --disableFastRender",
    "start-examplesite": "hugo server -s exampleSite --themesDir=../.. --disableFastRender",
    "build-examplesite": "NODE_ENV=production hugo -s exampleSite --themesDir=../.. --gc",
    "build": "NODE_ENV=production hugo --gc"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.2"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
